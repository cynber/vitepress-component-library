{
  "name": "@cynber/vitepress-components",
  "version": "0.1.4",
  "description": "Collection of reusable Vue components tailored for use in VitePress projects",
  "main": "dist/vitepress-components.cjs.js",
  "module": "dist/vitepress-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/vitepress-components.es.js",
      "require": "./dist/vitepress-components.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cynber/vitepress-component-library.git"
  },
  "keywords": [
    "vitepress",
    "components",
    "vue",
    "vite"
  ],
  "author": "cynber",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/cynber/vitepress-component-library/issues"
  },
  "homepage": "https://github.com/cynber/vitepress-component-library#readme",
  "peerDependencies": {
    "vitepress": "^1.3.0",
    "vue": "^3.4.0",
    "@iconify/vue": "^3.0.0"
  },
  "devDependencies": {
    "@iconify/vue": "^3.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "vite": "^6.0.7"
  }
}