{
  "name": "@cynber/vitepress-components",
  "version": "0.1.5",
  "description": "Collection of reusable Vue components tailored for use in VitePress projects",
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vue-tsc && vite build",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cynber/vitepress-component-library.git"
  },
  "keywords": [
    "vitepress",
    "components",
    "vue",
    "vite"
  ],
  "author": "cynber",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/cynber/vitepress-component-library/issues"
  },
  "homepage": "https://github.com/cynber/vitepress-component-library#readme",
  "peerDependencies": {
    "@iconify/vue": "^3.0.0",
    "vitepress": "^1.3.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@vitejs/plugin-vue": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vue-tsc": "^2.2.0"
  }
}
